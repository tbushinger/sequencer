{
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "alwaysStrict": true,
        "declaration": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "pretty": true,
        "importHelpers": false,
        "inlineSourceMap": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "newLine": "LF",
        "noEmitOnError": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noLib": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": false,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "ES5",
        "lib": [
            "es2015",
            "es2016"
        ],
        "types": [
            "mocha",
            "chai",
            "node"
        ],
        "composite": true
    }
}